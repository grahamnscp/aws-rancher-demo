# Params.sh

# params used in deployment scripts

# aws access
SSH_USERNAME=ec2-user
SSH_KEY=/Users/myuser/.ssh/awsmyuser
SSH_OPTS="-o StrictHostKeyChecking=no -i $SSH_KEY"

# rke2
# https://github.com/rancher/rke2/releases
RKE2_TOKEN="myjointoken"
RKE2_VERSION="v1.31.8+rke2r1"
RKE2_VERSION_AI="v1.31.5+rke2r1"

# rancher
# helm search repo rancher-prime --versions
# https://ranchermanager.docs.rancher.com/versions
RANCHERVERSION="2.10.2"
BOOTSTRAPADMINPWD=mybootistrapadminpwd
RANCHERADMINPWD=myadminpwd

# downstream cluster1 & cluster3
STORAGE_DEV1="nvme1n1"
STORAGE_DEV2="nvme2n1"

# SUSE Observability
OBS_LICENSE="MY-SUSE-OBS-LICENCE-KEY-HERE"
OBS_HOSTNAME=obs.domainname
OBS_ADMIN_PWD=mysuseobsadminpwd
OBS_SERVICE_TOKEN=mytokenvalue

# SUSE Security
SEC_HOSTNAME=sec.domainname
SEC_ADMIN_PWD=mysusesecurityadminpwd
# helm search repo neuvector --versions
SEC_NV_APP_VERSION=5.4.4

# SUSE Application Collection
APPCOL_USER="my@emaildomain.com"
APPCOL_TOKEN="XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX=="

# SUSE AI
AI_HOSTNAME=ai.domainname

